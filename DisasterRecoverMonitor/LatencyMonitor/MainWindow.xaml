        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LatencyMonitor"
        xmlns:visualFx="http://mixmodes.com/visualFx" 
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <!--DataTemplate DataType="{x:Type local:dbConnect}">
            <local:dbConnect />
            
        </-->
        
    </Window.Resources>




    <Grid x:Name="dbGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TreeView Name="tree" Grid.Column="0"/>
        <!--<Frame x:Name="mainFrame" Grid.Column="1">
            
        </Frame>-->
        <DockPanel x:Name="foPanel" Grid.Column="1" AllowDrop="True">

        </DockPanel>
        
    </Grid>
</Window>
